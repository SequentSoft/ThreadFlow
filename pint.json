{
    "preset": "psr12",
    "rules": {
        "concat_space": {
            "spacing": "one"
        },
        "phpdoc_align": {
            "align": "left"
        },
        "type_declaration_spaces": {
            "elements": ["function", "property"]
        },
        "not_operator_with_successor_space": true
    }
}
